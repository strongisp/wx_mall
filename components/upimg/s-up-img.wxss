/* components/upimg/s-up-img.wxss */

.up_img_wrap {
    width: 90rpx;
    height: 90rpx;
    position: relative;
}

.up_img_wrap image {
    width: 100%;
    height: 100%;
    border-radius: 15rpx;
    border: 1rpx solid #eee;
}

.up_img_wrap icon {
    position: absolute;
    top: -18rpx;
    right: -18rpx;
}